{
  "network": {
    "project": "playground-s-11-cfff98e5",
    "region": "us-central1",
    "network_name": "auto-vpc",
    "subnet_name": "auto-subnet",
    "subnet_cidr": "10.2.0.0/24"
  },

  "subnetwork": {
    "project": "playground-s-11-cfff98e5",
    "region": "us-central1",
    "network_id": "projects/playground-s-11-cfff98e5/global/networks/auto-vpc",
    "subnets": [
      {"name": "app-subnet", "cidr": "10.30.1.0/24", "region": "us-central1"},
      {"name": "db-subnet", "cidr": "10.30.2.0/24", "region": "us-east1"}
    ]
  },

  "storage_bucket": {
    "project": "playground-s-11-cfff98e5",
    "region": "US",
    "buckets": [
      {
        "name": "python-managed-bucket",
        "location": "US",
        "force_destroy": true,
        "uniform_bucket_level_access": true,
        "versioning": true,
        "lifecycle_rules": [
          {"action": "Delete", "age": 90}
        ]
      }
    ]
  },

  "firewall": {
    "project": "playground-s-11-cfff98e5",
    "region": "us-central1",
    "network_id": "projects/playground-s-11-cfff98e5/global/networks/auto-vpc",
    "firewall_rules": [
      {
        "name": "allow-ssh",
        "direction": "INGRESS",
        "protocol": "tcp",
        "ports": ["22"],
        "source_ranges": ["0.0.0.0/0"],
        "priority": 1000
      },
      {
        "name": "allow-internal",
        "direction": "INGRESS",
        "protocol": "all",
        "source_ranges": ["10.0.0.0/16"],
        "priority": 1001
      }
    ]
  },

  "cloudsql": {
    "project": "playground-s-11-cfff98e5",
    "region": "us-central1",
    "db_instance_name": "my-sql-instance",
    "db_version": "POSTGRES_14",
    "tier": "db-f1-micro",
    "disk_size": 20,
    "database_name": "appdb",
    "user": "appuser",
    "password": "securepassword123"
  },

  "compute_instance": {
    "project": "playground-s-11-cfff98e5",
    "region": "us-central1",
    "zone": "us-central1-a",
    "instances": [
      {
        "name": "web-instance",
        "machine_type": "e2-medium",
        "disk_size": 20,
        "image": "debian-cloud/debian-11",
        "network": "auto-vpc",
        "subnetwork": "app-subnet",
        "tags": ["web", "dev"]
      },
      {
        "name": "db-instance",
        "machine_type": "e2-small",
        "disk_size": 50,
        "image": "debian-cloud/debian-11",
        "network": "auto-vpc",
        "subnetwork": "db-subnet",
        "tags": ["db"]
      }
    ]
  },

  "gke_cluster": {
    "project": "playground-s-11-cfff98e5",
    "region": "us-central1",
    "cluster_name": "my-gke-cluster",
    "network": "projects/playground-s-11-cfff98e5/global/networks/auto-vpc",
    "subnetwork": "app-subnet",
    "node_count": 3,
    "machine_type": "e2-medium",
    "disk_size_gb": 30
  },

  "pubsub": {
    "project": "playground-s-11-cfff98e5",
    "topics": [
      {
        "name": "user-events",
        "subscriptions": [
          {"name": "user-events-sub", "ack_deadline_seconds": 20}
        ]
      }
    ]
  },

  "loadbalancer": {
    "project": "playground-s-11-cfff98e5",
    "region": "us-central1",
    "backend_service_name": "my-backend",
    "health_check_port": 80,
    "forwarding_rule_name": "http-rule",
    "ip_protocol": "TCP",
    "target_port": 80
  },

  "iam": {
    "project": "playground-s-11-cfff98e5",
    "bindings": [
      {
        "role": "roles/viewer",
        "members": ["user:viewer@example.com"]
      },
      {
        "role": "roles/storage.admin",
        "members": ["serviceAccount:svc-123@playground-s-11-cfff98e5.iam.gserviceaccount.com"]
      }
    ]
  },

  "cloudrun": {
    "project": "playground-s-11-cfff98e5",
    "region": "us-central1",
    "service_name": "my-cloudrun-service",
    "image": "gcr.io/playground-s-11-cfff98e5/my-app:latest",
    "allow_unauthenticated": true
  },

  "cdn_and_dns": {
    "project": "playground-s-11-cfff98e5",
    "region": "us-central1",
    "dns_managed_zone": "my-zone",
    "dns_name": "example.com.",
    "cdn_backend_bucket": "python-managed-bucket",
    "dns_records": [
      {"name": "www", "type": "A", "ttl": 300, "rrdatas": ["34.120.0.1"]}
    ]
  }
}
